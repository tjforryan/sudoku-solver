{
  "name": "seed-app",
  "version": "0.0.1",
  "description": "A seed app that can form the foundation for other apps",
  "author": "Tom",
  "contributors": [
    {
      "name": "Tom Forryan",
      "email": "tjforryan@yahoo.co.uk"
    }
  ],
  "bin": {},
  "scripts": {
    "start": "npm run build && NODE_ENV=production node server",
    "dev": "NODE_ENV=development node server",
    "test": "gulp unitTests",
    "build": "npm run clean && mkdir build && npm run build-bundle",
    "build-bundle": "cp src/index.html build/ && NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rimraf ./build",
    "lint": "eslint '**/*.js' '**/*.jsx'",
    "lintFix": "eslint --fix '**/*.js' '**/*.jsx'"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-loader": "~7.0.0",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-polyfill": "~6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "~6.24.1",
    "css-loader": "~0.28.0",
    "express": "~4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "path": "~0.12.7",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "~15.5.4",
    "react-dom": "~15.5.4",
    "react-router": "~4.1.1",
    "react-transform-catch-errors": "~1.0.2",
    "react-transform-hmr": "~1.0.4",
    "rimraf": "~2.6.1",
    "style-loader": "~0.16.1",
    "webpack": "~2.4.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "babel-eslint": "~7.2.3",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~4.0.0",
    "eslint-plugin-no-only-tests": "~1.1.0",
    "eslint-plugin-react": "~6.10.0",
    "expect": "~1.20.2",
    "gulp": "~3.9.0",
    "gulp-mocha": "~4.3.0",
    "redbox-react": "~1.3.0",
    "vinyl-source-stream": "~1.1.0"
  },
  "private": true,
  "analyze": true,
  "keywords": [
    "sudoku"
  ]
}
